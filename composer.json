{
  "name": "pemad-intl/main-api-service",
  "description": "Main API client for Pemad apps (signature, retry, cache, rate-limit).",
  "type": "library",
  "license": "MIT",
  "authors": [
    { "name": "Danang Pemad", "email": "danang@pemad.or.id" }
  ],
  "autoload": {
    "psr-4": {
      "Pemad\\MainApi\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pemad\\MainApi\\MainApiServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10|^11",
    "illuminate/http": "^10|^11",
    "illuminate/cache": "^10|^11"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
